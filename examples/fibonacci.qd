// Calculate the nth Fibonacci number
// build: quadc -o fibonacci fibonacci.qd
// usage: echo <number> | ./fibonacci
// example: echo 9 | ./fibonacci

// Fn = Fn-1 + Fn-2, for n > 1
fn fibonacci() {
	push 1
	jg run
	pop
	return

run:
	pop
	dup
	dec
	fibonacci
	swap
	push 2
	sub
	fibonacci
	swap
	add
}

fn main() {
	read
	fibonacci
	print
}
