struct Vec2 {
  x:f64
  y:f64
}

fn test_operations( -- ) {
  5.0 10.0 Vec2 -> v
  defer { v free }

  // Field values can now be used in operations!
  v @x v @y add . nl

  // Multiple operations
  v @x 2.0 mul v @y add . nl

  // Comparison
  v @x v @y gt
  if {
    "x > y" print nl
  } else {
    "x <= y" print nl
  }
}

fn main( -- ) {
  test_operations
}
