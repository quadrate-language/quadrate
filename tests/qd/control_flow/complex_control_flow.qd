fn main( -- ) {
	// Test if/else with comparisons
	// 10 < 20, so should print nl 100
	10 20 lt if {
		100 . nl
	} else {
		200 . nl
	}
	// 10 >= 5, so should print nl 400
	10 5 lt if {
		300 . nl
	} else {
		400 . nl
	}
	// Nested if/else
	// 5 > 3, so should print nl 500
	1 if {
		5 3 gt if {
			500 . nl
		} else {
			600 . nl
		}
	}
	// For loop with conditionals
	// Should print nl 1000 when $ == 2
	0 5 1 for {
		$
		2 eq if {
			1000 . nl
		}
	}
	// For loop with break
	// Should print nl: 0 1 2 3 4
	0 10 1 for {
		$
		$
		5 eq if {
			break
		}
	 . nl
	}
	// Nested for loops with conditionals
	// Should print nl 99 three times (when inner loop $ == 1)
	0 3 1 for {
		0 2 1 for {
			$
			1 eq if {
				99 . nl
			}
		}
	}
}
