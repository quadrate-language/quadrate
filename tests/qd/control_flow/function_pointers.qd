fn greet( -- ) {
	"Hello from greet!" print nl
}

fn farewell( -- ) {
	"Goodbye from farewell!" print nl
}

fn pushNumber( -- ) {
	42 printv
}

fn main( -- ) {
	// Test 1: Push function pointer and check depth
	&greet
	depth
	printv
	// Should print nl: int:1
	clear
	// Test 2: Call greet via function pointer
	&greet
	call
	// Should print nl: "Hello from greet!"
	// Test 3: Call farewell via function pointer
	&farewell
	call
	// Should print nl: "Goodbye from farewell!"
	// Test 4: Call pushNumber which pushes a value
	&pushNumber
	call
	// Should print nl: int:42
	// Test 5: Multiple function pointers on stack
	&greet
	&farewell
	depth
	printv
	// Should print nl: int:2
	call
	// Calls farewell
	call
	// Calls greet
}
