// Test: complex logical conditions

use bits

fn main() {
	// Test: (true AND true) OR false = true
	1 1 bits::and 0 bits::or . nl

	// Test: (false OR true) AND true = true
	0 1 bits::or 1 bits::and . nl

	// Test: NOT (true AND false) = true
	1 0 bits::and bits::not . nl

	// Test: (true OR false) AND (false OR true) = true
	1 0 bits::or 0 1 bits::or bits::and . nl

	// Test: NOT (false OR false) = true
	0 0 bits::or bits::not . nl

	// Test: (10 > 5) AND (3 < 7) = true
	10 5 gt 3 7 lt bits::and . nl

	// Test: (10 == 10) OR (5 > 10) = true
	10 10 eq 5 10 gt bits::or . nl

	// Test: (5 < 3) AND (10 == 10) = false
	5 3 lt 10 10 eq bits::and . nl
}
