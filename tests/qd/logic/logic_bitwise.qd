fn main( -- ) {
	// Test and - bitwise AND
	12 10 and .
	// Should print: 8 (0b1100 & 0b1010 = 0b1000)
	15 7 and .
	// Should print: 7 (0b1111 & 0b0111 = 0b0111)
	// Test or - bitwise OR
	12 10 or .
	// Should print: 14 (0b1100 | 0b1010 = 0b1110)
	8 4 or .
	// Should print: 12 (0b1000 | 0b0100 = 0b1100)
	// Test xor - bitwise XOR
	12 10 xor .
	// Should print: 6 (0b1100 ^ 0b1010 = 0b0110)
	15 7 xor .
	// Should print: 8 (0b1111 ^ 0b0111 = 0b1000)
	5 5 xor .
	// Should print: 0 (same values XOR to 0)
	// Test not - bitwise NOT
	0 not .
	// Should print: -1 (all bits set)
	-1 not .
	// Should print: 0
	5 not .
	// Should print: -6
	// Test lshift - logical shift left
	1 3 lshift .
	// Should print: 8 (1 << 3 = 0b1000)
	5 2 lshift .
	// Should print: 20 (5 << 2 = 0b10100)
	7 4 lshift .
	// Should print: 112 (7 << 4 = 0b1110000)
	// Test rshift - logical shift right
	8 3 rshift .
	// Should print: 1 (8 >> 3 = 0b1)
	20 2 rshift .
	// Should print: 5 (20 >> 2 = 0b101)
	112 4 rshift .
	// Should print: 7 (112 >> 4 = 0b111)
}
