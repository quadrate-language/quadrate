// Import test - using imports with defer blocks

import "libstdfmtqd_static.a" as "fmt" {
    fn printf(format:s -- )
}

fn print_defer() {
    "Defer executed\n" fmt::printf
}

fn test_defer() {
    defer {
        print_defer
    }
    "Before defer\n" fmt::printf
}

fn main() {
    "Start\n" fmt::printf
    test_defer
    "End\n" fmt::printf
}
