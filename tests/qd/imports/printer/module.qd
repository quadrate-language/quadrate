// Module that uses imports internally

import "libstdqd.so" as "stdqd" {
    fn printf(format:s -- )
}

fn print_number(num:i -- ) {
    "Number: %d\n" swap stdqd::printf
}

fn print_message(msg:s -- ) {
    "Message: %s\n" swap stdqd::printf
}

fn main() {
    123 print_number
    "Testing printer module" print_message
}
