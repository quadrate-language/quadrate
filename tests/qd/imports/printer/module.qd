// Module that uses imports internally

import "libstdfmtqd_static.a" as "fmt" {
    fn printf(format:str -- )
}

pub fn print_number(num:i64 -- ) {
    "Number: %d\n" fmt::printf
}

pub fn print_message(msg:str -- ) {
    "Message: %s\n" fmt::printf
}

pub fn main() {
    123 print_number
    "Testing printer module" print_message
}
