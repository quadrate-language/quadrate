// Module that uses imports internally

import "libstdfmtqd_static.a" as "fmt" {
    fn printf(format:s -- )
}

pub fn print_number(num:i -- ) {
    "Number: %d\n" swap fmt::printf
}

pub fn print_message(msg:s -- ) {
    "Message: %s\n" swap fmt::printf
}

pub fn main() {
    123 print_number
    "Testing printer module" print_message
}
