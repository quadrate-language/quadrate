// Import test - complex format strings

import "libstdqd.so" as "stdqd" {
    fn printf(format:s -- )
}

fn main() {
    // Multiple arguments
    "Name: %s, Age: %d, Height: %f\n" "Alice" 25 1.75 stdqd::printf

    // Integer and float
    "Int: %d, Float: %f\n" 42 3.14159 stdqd::printf

    // Percent literal
    "100%% complete\n" stdqd::printf

    // Multiple strings
    "%s %s!\n" "Hello" "World" stdqd::printf
}
