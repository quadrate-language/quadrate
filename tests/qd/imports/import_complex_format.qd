// Import test - complex format strings

import "libstdqd.so" as "stdqd" {
    fn printf(format:s -- )
}

fn main() {
    // Multiple arguments
    "Alice" 25 1.75 "Name: %s, Age: %d, Height: %f\n" stdqd::printf

    // Integer and float
    42 3.14159 "Int: %d, Float: %f\n" stdqd::printf

    // Percent literal
    "100%% complete\n" stdqd::printf

    // Multiple strings
    "Hello" "World" "%s %s!\n" stdqd::printf
}
