// Import test - complex format strings

import "libstdfmtqd_static.a" as "fmt" {
    fn printf(format:s -- )
}

fn main() {
    // Multiple arguments
    "Name: %s, Age: %d, Height: %f\n" "Alice" 25 1.75 fmt::printf

    // Integer and float
    "Int: %d, Float: %f\n" 42 3.14159 fmt::printf

    // Percent literal
    "100%% complete\n" fmt::printf

    // Multiple strings
    "%s %s!\n" "Hello" "World" fmt::printf
}
