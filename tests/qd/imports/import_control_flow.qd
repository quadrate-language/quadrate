// Import test - using imports in control flow

import "libstdqd.so" as "stdqd" {
    fn printf(format:s -- )
}

fn print_if() {
    "Inside if block\n" stdqd::printf
}

fn print_loop() {
    "Loop iteration\n" stdqd::printf
}

fn print_case() {
    "Case 2\n" stdqd::printf
}

fn main() {
    // In if statement
    1 1 eq
    if {
        print_if
    }

    // In for loop
    0 3 1 for {
        print_loop
    }

    // In switch
    2 switch {
        case 2 {
            print_case
        }
    }
}
