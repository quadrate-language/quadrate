fn main() {
	// Test tuck: ( a b -- b a b )
	10 20 tuck
	prints
	clear

	// Test pick: copy nth element (0-indexed from top)
	100 200 300 400
	2 pick  // Copy element at index 2 (which is 200)
	prints
	clear

	// Test roll: rotate n elements
	10 20 30 40
	3 roll  // Rotate top 3: 20 30 40 -> 30 40 20
	prints
	clear

	// Test swap2: swap top two pairs
	1 2 3 4
	swap2  // Swap pairs: ( 1 2 3 4 -- 3 4 1 2 )
	prints
	clear

	// Test over2: copy second pair to top
	5 6 7 8
	over2  // Copy second pair (5 6) to top
	prints
}
