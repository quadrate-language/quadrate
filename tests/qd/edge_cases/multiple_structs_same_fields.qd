struct Point2D {
  x:f64
  y:f64
}

struct Point3D {
  x:f64
  y:f64
  z:f64
}

struct Vec2 {
  x:f64
  y:f64
}

fn main( -- ) {
  1.0 2.0 Point2D -> p2
  defer { p2 free }

  3.0 4.0 5.0 Point3D -> p3
  defer { p3 free }

  6.0 7.0 Vec2 -> v
  defer { v free }

  p2 @x . nl
  p3 @z . nl
  v @y . nl
}
