struct Point {
  x:f64
  y:f64
}

fn main( -- ) {
  0 10 1 for {
    $ 2 mod 0 eq
    if {
      1.0 2.0 Point -> p
      defer { p free }
      0 5 1 for {
        $ 3 lt
        if {
          p @x . nl
        } else {
          p @y . nl
        }
      }
    } else {
      "Odd" prints nl
    }
  }
}
