use math

fn main( -- ) {
	// Test ln (natural logarithm)
	1 math::ln . nl
	// ln(1) = 0
	2.718281828 math::ln . nl
	// ln(e) ≈ 1
	10 math::ln . nl
	// ln(10) ≈ 2.302585
	// Test log10 (base 10 logarithm)
	1 math::log10 . nl
	// log10(1) = 0
	10 math::log10 . nl
	// log10(10) = 1
	100 math::log10 . nl
	// log10(100) = 2
	1000 math::log10 . nl
	// log10(1000) = 3
}
