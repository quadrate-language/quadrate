fn main( -- ) {
	// Test sq (square)
	5 sq . nl
	// Should print nl: 25
	-3 sq . nl
	// Should print nl: 9
	// Test sqrt (square root)
	16 sqrt . nl
	// Should print nl: 4
	25 sqrt . nl
	// Should print nl: 5
	// Test cb (cube)
	3 cb . nl
	// Should print nl: 27
	-2 cb . nl
	// Should print nl: -8
	// Test cbrt (cube root)
	27 cbrt . nl
	// Should print nl: 3
	8 cbrt . nl
	// Should print nl: 2
	// Test abs (absolute value)
	-42 abs . nl
	// Should print nl: 42
	42 abs . nl
	// Should print nl: 42
	// Test inv (inverse/reciprocal)
	// 1/2 = 0.5
	2 inv . nl
	// Should print nl: 0.5
	4 inv . nl
	// Should print nl: 0.25
	// Test inc/dec
	5 inc . nl
	// Should print nl: 6
	5 dec . nl
	// Should print nl: 4
	// Test fac (factorial)
	5 fac . nl
	// Should print nl: 120
	0 fac . nl
	// Should print nl: 1
	1 fac . nl
	// Should print nl: 1
	// Test ceil/floor
	3.2 ceil . nl
	// Should print nl: 4
	3.8 floor . nl
	// Should print nl: 3
	-2.3 ceil . nl
	// Should print nl: -2
	-2.7 floor . nl
	// Should print nl: -3
	// Test pow (exponentiation)
	2 10 pow . nl
	// Should print nl: 1024 (2^10)
	3 3 pow . nl
	// Should print nl: 27 (3^3)
	5 2 pow . nl
	// Should print nl: 25 (5^2)
	2 0 pow . nl
	// Should print nl: 1 (2^0)
	// Test round (round to nearest integer)
	2.5 round . nl
	// Should print nl: 3
	2.4 round . nl
	// Should print nl: 2
	-2.5 round . nl
	// Should print nl: -2
	3.7 round . nl
	// Should print nl: 4
}
