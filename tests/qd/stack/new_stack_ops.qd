fn main() {
	// Test drop - removes top element
	10 20 30 drop
	. .  // Should print 20, then 10

	// Test drop2 - removes top 2 elements
	100 200 300 400 drop2
	. .  // Should print 200, then 100

	// Test rot - rotate top 3 elements
	1 2 3 rot
	. . .  // Should print 1 (moved to top), 3, 2

	// Test mod - modulo operation
	17 5 mod .  // Should print 2
	10 3 mod .  // Should print 1

	// Test neg - negate
	42 neg .  // Should print -42
	-10 neg .  // Should print 10
	3.14 neg .  // Should print -3.14

	// Test min - minimum of two values
	10 20 min .  // Should print 10
	5 3 min .  // Should print 3
	3.5 2.7 min .  // Should print 2.7

	// Test max - maximum of two values
	10 20 max .  // Should print 20
	5 3 max .  // Should print 5
	3.5 2.7 max .  // Should print 3.5

	// Test and - bitwise AND
	12 10 and .  // Should print 8 (0b1100 & 0b1010 = 0b1000)
	15 7 and .  // Should print 7 (0b1111 & 0b0111 = 0b0111)

	// Test or - bitwise OR
	12 10 or .  // Should print 14 (0b1100 | 0b1010 = 0b1110)
	8 4 or .  // Should print 12 (0b1000 | 0b0100 = 0b1100)

	// Test not - bitwise NOT
	0 not .  // Should print -1 (all bits set)
	-1 not .  // Should print 0
	5 not .  // Should print -6

	// Test combining operations
	100 50 min 25 max .  // min(100, 50) = 50, max(50, 25) = 50

	// Test with mixed types (type promotion)
	5 3.5 max .  // Should print 5 (promoted to float)
	10 7.5 min .  // Should print 7.5
}
